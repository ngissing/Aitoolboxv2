  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "dotenv -e .env -- tsx server/index.ts",
    "install:all": "npm-run-all install:root install:client",
    "install:root": "npm install",
    "install:client": "cd client && npm install",
    "build:client": "cd client && npm run build",
    "build:server": "tsc --project tsconfig.server.json",
    "bundle:server": "esbuild server/index.ts --platform=node --bundle --format=esm --outdir=dist --external:lightningcss",
    "bundle:server:vercel": "esbuild server/index.ts --platform=node --bundle --format=esm --outfile=dist/server.js --external:express --external:@supabase/supabase-js --external:lightningcss --external:react --external:react-dom --external:pg --external:postgres --external:passport --external:passport-local --external:express-session --external:connect-pg-simple --external:memorystore --external:zod --minify --sourcemap",
    "build": "npm-run-all install:all build:client build:server bundle:server",
    "build:vercel": "npm-run-all install:all build:client build:server bundle:server:vercel",
    "start": "cross-env NODE_ENV=production node -r dotenv/config dist/server.js",
    "check": "tsc",
    "generate:types": "supabase gen types typescript --project-id nhjbqjvqorlavjfhuicy > shared/supabase.types.ts",
    "postinstall": "npm run install:client"
  },
